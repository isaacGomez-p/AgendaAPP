const { Capacitor } = require("@capacitor/core")

*********************************************************

Para generar apk con Capacitor

Borrar carpeta Android y Capacitor.config.ts

1. ionic build
2. npx cap init
2.1 EDIT CAPACITOR.CONFIG.TS
3. npx cap add android
4. npx cap sync 
5. npx cap open android

//En el AndroidManifest.xml a√±adir 
<application
    ...
    android:usesCleartextTraffic="true"
    ...
</application>


Capacitor.
,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  }
};